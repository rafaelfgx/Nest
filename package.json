{
    "name": "nest",
    "version": "1.0.0",
    "scripts": {
        "restore": "npm install",
        "update": "npm install -g npm-check-updates && ncu -u",
        "check": "prettier src --config .prettierrc --check && eslint",
        "fix": "prettier src --config .prettierrc --write && eslint --fix",
        "start": "nest start --watch --debug",
        "test": "jest --no-cache --coverage --detectOpenHandles",
        "build": "nest build"
    },
    "jest": {
        "testRegex": ".spec.ts",
        "transform": {
            ".ts": "ts-jest"
        }
    },
    "dependencies": {
        "@nestjs/cache-manager": "3.0.1",
        "@nestjs/common": "11.1.6",
        "@nestjs/config": "4.0.2",
        "@nestjs/core": "11.1.6",
        "@nestjs/jwt": "11.0.0",
        "@nestjs/platform-express": "11.1.6",
        "@nestjs/swagger": "11.2.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.2",
        "helmet": "8.1.0",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.2",
        "swagger-ui-express": "5.0.1"
    },
    "devDependencies": {
        "@eslint/js": "9.35.0",
        "@nestjs/cli": "11.0.10",
        "@nestjs/schematics": "11.0.7",
        "@nestjs/testing": "11.1.6",
        "@types/express": "5.0.3",
        "@types/jest": "30.0.0",
        "@types/node": "24.5.2",
        "@types/supertest": "6.0.3",
        "eslint": "9.35.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.4",
        "globals": "16.4.0",
        "jest": "30.1.3",
        "prettier": "3.6.2",
        "source-map-support": "0.5.21",
        "supertest": "7.1.4",
        "ts-jest": "29.4.3",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.9.2",
        "typescript-eslint": "8.44.0"
    }
}
